{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "gatey/authenticator",
  "version": "1.2.0",
  "title": "Authenticator",
  "category": "wpsuite-gatey",
  "description": "Gatey Authenticator",
  "attributes": {
    "screen": {
      "type": "string"
    },
    "variation": {
      "type": "string"
    },
    "colorMode": {
      "type": "string"
    },
    "showOpenButton": {
      "type": "boolean"
    },
    "openButtonTitle": {
      "type": "string"
    },
    "signingInMessage": {
      "type": "string"
    },
    "signingOutMessage": {
      "type": "string"
    },
    "redirectingMessage": {
      "type": "string"
    },
    "language": {
      "type": "string"
    },
    "direction": {
      "type": "string"
    }
  },
  "supports": {
    "inserter": true,
    "align": true,
    "html": false,
    "shadow": true,
    "spacing": { "margin": true, "padding": true }
  },
  "allowedBlocks": ["gatey/custom-block"],
  "textdomain": "auth-with-amazon-cognito",
  "editorScript": "file:./index.tsx",
  "editorStyle": "file:./index.css",
  "style": "file:./view.css",
  "render": "file:./render.php",
  "viewScript": "file:./view.tsx"
}
