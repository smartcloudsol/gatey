{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "gatey/form-field",
  "version": "1.0.0",
  "title": "Form Field",
  "category": "wpsuite-gatey",
  "description": "Form Field for Gatey Authenticator - display a form field on sign-up or edit-account page.",
  "attributes": {
    "attribute": {
      "type": "string"
    },
    "custom": {
      "type": "string"
    },
    "required": {
      "type": "boolean"
    },
    "hidden": {
      "type": "boolean"
    },
    "type": {
      "type": "string"
    },
    "label": {
      "type": "string"
    },
    "labelHidden": {
      "type": "boolean"
    },
    "placeholder": {
      "type": "string"
    },
    "autocomplete": {
      "type": "string"
    },
    "defaultValue": {
      "type": "string"
    },
    "defaultChecked": {
      "type": "boolean"
    },
    "dialCode": {
      "type": "string"
    },
    "dialCodeList": {
      "type": "array"
    },
    "countryCode": {
      "type": "string"
    },
    "countryCodeList": {
      "type": "array"
    },
    "values": {
      "type": "array",
      "default": [],
      "items": {
        "type": "object",
        "properties": {
          "value": { "type": "string" },
          "label": { "type": "string" }
        }
      }
    }
  },
  "usesContext": ["gatey/custom-block/component"],
  "editorScript": "file:./index.tsx",
  "editorStyle": "file:./index.css",
  "style": "file:./index.css",
  "render": "file:./render.php"
}
