{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "gatey/authenticator",
  "version": "2.0.1",
  "title": "Authenticator",
  "category": "wpsuite-gatey",
  "description": "Gatey Authenticator",
  "attributes": {
    "screen": {
      "type": "string"
    },
    "variation": {
      "type": "string"
    },
    "colorMode": {
      "type": "string"
    },
    "showOpenButton": {
      "type": "boolean"
    },
    "openButtonTitle": {
      "type": "string"
    },
    "signingInMessage": {
      "type": "string"
    },
    "signingOutMessage": {
      "type": "string"
    },
    "redirectingMessage": {
      "type": "string"
    },
    "language": {
      "type": "string"
    },
    "direction": {
      "type": "string"
    },
    "totpIssuer": {
      "type": "string"
    },
    "uid": {
      "type": "string"
    },
    "customCSS": {
      "type": "string",
      "default": "selector {\n\t\n}"
    }
  },
  "supports": {
    "inserter": true,
    "align": true,
    "html": false,
    "shadow": true,
    "spacing": {
      "margin": true,
      "padding": true
    }
  },
  "allowedBlocks": [
    "gatey/custom-block"
  ],
  "textdomain": "gatey",
  "editorScript": "file:./index.js",
  "style": "file:./index.css",
  "render": "file:./render.php",
  "viewScript": "file:./view.js"
}